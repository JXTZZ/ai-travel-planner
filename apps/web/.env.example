# 基础配置
VITE_APP_NAME=LoTus'AI assistant
VITE_SUPABASE_URL=<your-supabase-url>
VITE_SUPABASE_ANON_KEY=<your-supabase-anon-key>

# 高德地图浏览器端 JS API key（需启用 JS API 服务）
VITE_AMAP_WEB_KEY=<your-amap-js-api-key>

# 高德地图安全密钥（JS API 安全密钥，可在控制台查看）
VITE_AMAP_SECURITY_JS_CODE=<your-amap-security-js-code>

# 高德地图 Web 服务 REST API key（用于地理编码兜底请求）
VITE_AMAP_REST_KEY=<your-amap-rest-key>

# 科大讯飞 Web API 凭证（用于语音助手 Edge Function 鉴权）
VITE_IFLYTEK_APP_ID=<your-iflytek-app-id>
VITE_IFLYTEK_API_KEY=<your-iflytek-api-key>
VITE_IFLYTEK_API_SECRET=<your-iflytek-api-secret>
